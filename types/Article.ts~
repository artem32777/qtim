export type Article = {
  id: string
  description: string
  image: string
  preview: string
  title: string
  createdAt: string
}

export function isArticle(obj: unknown): obj is Article {
  return (
      typeof obj === "object" &&
      obj !== null &&
      "id" in obj &&
      "description" in obj &&
      "title" in obj &&
      typeof (obj as Article).id === "string" &&
      typeof (obj as Article).description === "string" &&
      typeof (obj as Article).title === "string"
  )
}